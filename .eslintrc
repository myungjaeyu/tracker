{
    "plugins": ["@typescript-eslint"],
    "env":{                         
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
            "pragma": "React",
            "version": "detect"
        }
    },
    "parser":  "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "ecmaFeatures":{ "jsx": true }
    },
    "rules": {},
    "extends": [
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ]
}